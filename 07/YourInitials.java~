public class YourInitials {
    public static void main(String [] args) {
        System.out.println("K   K  W   W");
        System.out.println("K  K   W   W");
        System.out.println("K K    W   W");
        System.out.println("KK     W W W");
        System.out.println("K K    WW WW");
        System.out.println("K  K   WW WW");
        System.out.println("K   K  W   W");	
    }
}
